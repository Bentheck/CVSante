@model IEnumerable<CVSante.Models.UserMedication>

@{
    ViewData["Title"] = "Médication";
}

<h2>@ViewData["Title"]</h2>


<div class="form-container">
    <h3>Médication</h3>
    <div class="form-group">
        <label for="nom">Nom</label>
        <input type="text" id="nom" name="Nom" class="form-control" value="@Model">
    </div>
    <div class="form-group">
        <label for="medicamentProduitNat">Produit</label>
        <input type="text" id="medicamentProduitNat" name="MedicamentProduitNat" class="form-control" value="@Model">
    </div>
    <div class="form-group">
        <label for="posologie">Posologie</label>
        <input type="text" id="posologie" name="Posologie" class="form-control" value="@Model">
    </div>
    <div class="form-group">
        <label for="raison">Raison</label>
        <input type="text" id="raison" name="Raison" class="form-control" value="@Model">
    </div>
</div>

